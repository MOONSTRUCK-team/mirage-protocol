type Message = record {
  id: text;
  nonce: nat64;
  op_type: nat8;
  src_chain_id: nat64;
  dest_chain_id: nat64;
  dest_address: text;
  contract_address: text;
  token_id: nat64;
};

type ExecuteError = variant {
  HttpError: record {
    rejection_code: nat32;
    message: text;
  };
};

service : {
  notify_external_service: (Message) -> (variant { Ok: null; Err: ExecuteError });
}
